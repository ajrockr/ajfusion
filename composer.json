{
    "name": "ajrockr/ajfusion",
    "description": "A symfony bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajrockr\\Ajfusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajrockr\\Ajfusion\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Anthony Rizzo",
            "email": "anjorizz@gmail.com"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^7.2",
        "symfony/security-core": "^7.2",
        "symfony/security-bundle": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/validator": "^7.2",
        "symfony/form": "^7.2",
        "symfony/console": "^7.2",
        "symfony/asset": "^7.2",
        "symfony/serializer": "^7.2"
    },
    "require-dev": {
        "symfony/http-kernel": "^7.2",
        "phpunit/phpunit": "^12.0",
        "symfony/maker-bundle": "^1.62",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.1",
        "doctrine/dbal": "^4.2",
        "doctrine/orm": "^3.3",
        "doctrine/doctrine-bundle": "^2.13"
    }
}
